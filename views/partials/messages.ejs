<% if(typeof errors!= 'undefined') { %>
<%    errors.forEach(function(error){ %>

    <script>
        alert('<%= error.msg %>');
        </script>
<%    })        %>
<% } %>

<% if(success_msg!= '') { %>
    
    <script>
        alert('<%= success_msg %>');
        </script>
<% } %>

<% if(error_msg != '') { %>
    
    <script>
        alert('<%= error.msg %>');
        </script>
<% } %> 
<% if(error != '') { %>
    
    <script>
        alert('<%= error %>');
        </script>
<% } %>